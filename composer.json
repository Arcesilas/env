{
    "name": "arcesilas/env",
    "description": "Read environment variables",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arcesilas",
            "email": "arcesilas@neutre.email"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "php-mock/php-mock-phpunit": "^2.4",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "Arcesilas\\Env\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcesilas\\Env\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/phpcs"
        ]
    }
}
